ass="cart__join"><view data-event-opts="{{[['tap',[['purchased',['$event']]]]]}}" class="cart__join__title" bindtap="__e">加入购物车</view></view><view class="cart__once"><view data-event-opts="{{[['tap',[['buy']]]]}}" class="cart__once__title" bindtap="__e">立即购买</view></view></view><block wx:if="{{flag}}"><view class="product"><block wx:if="{{flag}}"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="product__pop" bindtap="__e"></view></block><view class="product__bezel"><block wx:for="{{goods}}" wx:for-item="item" wx:for-index="index" wx:key="title"><view><block wx:if="{{item.id}}"><view><view><image class="product__bezel__photo" src="{{item.imgurl}}" mode="aspectFill"></image></view><view class="product__bezel__title">{{''+item.title+''}}</view><view class="product__bezel__price">{{''+item.price+''}}</view></view></block><block wx:if="{{item.id}}"><view class="product__bezel__shopp">购买数量<text class="product__bezel__shopp__stock">有货</text><view class="product__bezel__shopp__subtract"><text data-event-opts="{{[['tap',[['subtract',[-1]]]]]}}" class="product__bezel__shopp__subtract__minus" bindtap="__e">-</text><text class="product__bezel__shopp__subtract__count">{{item.count}}</text><text data-event-opts="{{[['tap',[['subtract',[1]]]]]}}" class="product__bezel__shopp__subtract__plus" bindtap="__e">+</text></view></view></block></view></block></view></view></block><block wx:if="{{flags}}"><view class="trolley"><block wx:if="{{flags}}"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="trolley__pop" bindtap="__e"></view></block><view class="trolley__list iconfont"><view class="guanli"><view data-event-opts="{{[['tap',[['checkall',['$event']]]]]}}" class="select" bindtap="__e"><text class="select__all"><rich-text nodes="{{checkal?'&#xe767;':'&#xe605;'}}"></rich-text></text>全选</view><view data-event-opts="{{[['tap',[['empty',['$event']]]]]}}" class="remove" bindtap="__e">清除购物车</view></view><block wx:for="{{shoppings}}" wx:for-item="item" wx:for-index="index" wx:key="price"><view class="trolley__box"><block wx:if="{{item.count>0}}"><view class="fud"><view data-event-opts="{{[['tap',[['selected',['$0'],[[['shoppings','price',item.price]]]]]]]}}" class="checks" bindtap="__e"><rich-text nodes="{{item.check?'&#xe767;':'&#xe605;'}}"></rich-text></view><image class="trolley__list__photo" src="{{item.imgurl}}"></image><view class="trolley__boxx"><view class="trolley__list__title">{{''+item.title+''}}</view><view class="trolley__list__price">{{''+item.price+" * "+item.count+'/件'}}</view><view class="trolley__list__zj">{{'共'+item.count+"件 /"+price+" "+item.add+'元'}}</view></view></view></block></view></block><view class="gross"><view>{{'合计：￥'+accounts+''}}</view><view data-event-opts="{{[['tap',[['leave',['$event']]]]]}}" class="gross__button" bindtap="__e">结算</view></view></view></view></block></view>