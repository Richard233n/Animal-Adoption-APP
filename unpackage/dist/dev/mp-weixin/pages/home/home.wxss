.nurture::after {
  content: "";
  width: 64rpx;
  border: solid 1rpx #fd843e;
  position: absolute;
  margin-top: 20rpx;
  margin-left: 16rpx;
}
.nurture::before {
  content: "";
  width: 64rpx;
  border: solid 1rpx #fd843e;
  position: absolute;
  margin-top: 20rpx;
  margin-left: -82rpx;
}
