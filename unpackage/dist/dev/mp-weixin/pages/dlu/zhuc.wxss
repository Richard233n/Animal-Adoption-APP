
.content {
	background: # #e8e8e8;
	background-image: url(data:image/jpeg;base64,);
	background-size: 100% 100%;
	background-repeat: no-repeat;
	height: 1300rpx;
}
.avatorWrapper {
	height: 30vh;
	width: 100vw;
	display: flex;
	justify-content: center;
	align-items: flex-end;
}
.avator {
	width: 200rpx;
	height: 200rpx;
	border-radius: 50%;
}
.form {
	padding: 0 100rpx;
	margin-top: 80rpx;
}
.inputWrapper {
	width: 100%;
	height: 80rpx;
	background: white;
	border-radius: 20px;
	box-sizing: border-box;
	padding: 0 20px;
	margin-top: 25px;
	border: 1px solid black;
}
.inputWrapper .input {
	width: 100%;
	height: 100%;
	text-align: center;
	font-size: 15px;
}
.loginBtn {
	width: 100%;
	height: 80rpx;
	background-image: linear-gradient(180deg, #fbc720 0%, #f83f02 100%);
	border-radius: 50rpx;
	margin-top: 50px;
	display: flex;
	justify-content: center;
	align-items: center;
}
.loginBtn .btnValue {
	color: white;
}
.forgotBtn {
	text-align: center;
	color: #000000;
	font-size: 15px;
	margin-top: 20rpx;
}

