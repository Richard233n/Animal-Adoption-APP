<view class="content"><view class="avatorWrapper"><view class=" iconfont"><image class="avator" src="{{images}}" mode="aspectFill" data-event-opts="{{[['tap',[['camera',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="form"><view class="inputWrapper"><input class="input" type="text" value="{{name}}" placeholder="请输入用户名" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" bindinput="__e"/></view><view class="inputWrapper"><input class="input" type="password" value="{{password}}" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="loginBtn" bindtap="__e"><text class="btnValue">登录</text></view><view data-event-opts="{{[['tap',[['enroll',['$event']]]]]}}" class="forgotBtn" bindtap="__e"><text>注册</text></view></view></view>