<view class="content"><view class="avatorWrapper"><view class=" iconfont">立即注册</view></view><view class="form"><view class="inputWrapper"><input class="input" type="text" value="{{site.name}}" placeholder="请输入2到8位中文名字" data-event-opts="{{[['blur',[['username',['$event']]]],['input',[['__set_model',['$0','name','$event',[]],['site']]]]]}}" bindblur="__e" bindinput="__e"/></view><view class="inputWrapper"><input class="input" type="password" value="{{site.password}}" placeholder="请输入6到20位字母或数字" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['site']],['code',['$event']]]]]}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['enroll',['$event']]]]]}}" class="loginBtn" bindtap="__e"><text class="btnValue">注册</text></view></view></view>