.pet {
  display: flex;
  margin: 60rpx 0 0 24rpx;
  width: 702rpx;
  height: 240rpx;
  border-radius: 18rpx;
  box-shadow: 0rpx 0rpx 34rpx 0rpx rgba(0, 0, 0, 0.08);
}
.pet__dog__print {
  width: 170rpx;
  height: 170rpx;
  background-color: #ffffff;
  border-radius: 15rpx;
  margin: 24rpx 0 0 20rpx;
}
.pet__dog__title {
  width: 452rpx;
  height: 60rpx;
  font-size: 26rpx;
  color: #222222;
  margin: 24rpx 0 0 20rpx;
}
.pet__dog__box {
  display: flex;
  justify-content: space-between;
  margin-top: 50rpx;
}
.pet__dog__box__ly {
  font-size: 22rpx;
  color: #f96c0d;
  margin-left: 20rpx;
}
.pet__dog__box__sc {
  font-size: 22rpx;
  color: #999999;
}
.pet__dog__box__liu {
  font-size: 22rpx;
  color: #999999;
}
